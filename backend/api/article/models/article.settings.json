{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "private": false,
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "date": {
      "type": "date",
      "required": true,
      "unique": false
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "summary": {
      "type": "text",
      "required": true,
      "unique": false
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "articles"
    },
    "content": {
      "type": "text",
      "required": true
    },
    "headline": {
      "type": "string"
    }
  }
}
